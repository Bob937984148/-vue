utton>\r\n                         \r\n                        </form>\r\n                      \r\n            \r\n              </br>\r\n            <input type=\"checkbox\" class=\"\"/> \r\n            <span class=\"l_sp2\">7天内免登录</span><br>\r\n            <div class=\"down_text\">\r\n                <span class=\"l_sp1\">试运行期间仅支持网易账号登录哦</span><br>\r\n                <span class=\"l_sp\">其他合作渠道账号正在紧张准备中，请少侠耐心等待</span>\r\n            </div>\r\n        </div>\r\n          </div> \r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n\r\n\r\n\r\n\r\n<script>\r\n import {alert} from \"mint-ui\";\r\n export default {\r\n   data(){\r\n     return {\r\n       name:\"\",\r\n       pwd:\"\",\r\n       lhshow:true\r\n     }\r\n   },\r\n   methods:{\r\n      indexshow(e){\r\n          if(e==1){\r\n              this.lhshow=true\r\n          }else if(e==2){\r\n              this.lhshow=false;\r\n          }\r\n      },\r\n     handleLogin(){\r\n       //1:获取用户输入 用户名和密码\r\n       var n = this.email;\r\n       var p = this.upwd;\r\n       //2:验证\r\n       var reg = /^[a-z0-9_]{3,12}$/i;\r\n       //3:验证失败提示消息\r\n       if(!reg.test(n)){\r\n         Toast(\"用户名格式不正确\");\r\n         return;\r\n       }\r\n       if(!reg.test(p)){\r\n         Toast(\"密码格式不正确\");\r\n         return;\r\n       }\r\n       //4:发送ajax请求\r\n       //console.log(n+\":\"+p);\r\n       var url = \"http://127.0.0.1:3000\";\r\n       url+=\"/login?email=\"+n;\r\n       url+=\"&upwd=\"+p\r\n       this.axios.get(url).then(result=>{\r\n           if(result.data.code == 1){\r\n               Toast(\"登录成功\");\r\n               this.$router.push(\"/Home\")\r\n           }else{\r\n              alert(\"用户名或密码不匹配\")\r\n           }\r\n       });\r\n       //5:如果登录成功 跳转/Home\r\n       //6:如果失败     提示消息\r\n     }\r\n   }\r\n }\r\n</script>\r\n\r\n<!--样式-->\r\n<style>\r\n*{margin:0;padding:0}\r\n#zong{\r\n    width:100%;\r\n    background:#f5f5f5;\r\n    \r\n\r\n}\r\n#container{\r\n    width:460px;\r\n    height:969